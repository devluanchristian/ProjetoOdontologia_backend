// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            String  @id @default(uuid())
  name          String
  email         String  @unique
  RA            BigInt?
  password_hash String

  user_Types    UserTypes? @relation(fields: [users_TypesId], references: [id])
  users_TypesId String

  @@map("users")
}

model UserTypes {
  id          String @id @default(uuid())
  description String
  User        User[]

  @@map("users_type")
}

model Menu {
  id          String @id @default(uuid())
  title       String
  description String
}
